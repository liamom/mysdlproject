<Window x:Class="sdk.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sdk"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="560.333">
    <Grid Margin="0,0,0,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="25*"/>
            <ColumnDefinition Width="113*"/>
        </Grid.ColumnDefinitions>
        <ToolBarPanel Height="41" VerticalAlignment="Top" Margin="10,10,10,0" Background="Black" Grid.Column="1">
            <TreeView x:Name="treeView" HorizontalAlignment="Left" Panel.ZIndex="5" />
        </ToolBarPanel>
        <Button x:Name="button" Content="Button" Margin="10,56,0,0" VerticalAlignment="Top" Click="btnClick" Height="20" HorizontalAlignment="Left" Width="50" Grid.Column="1"/>
        <TreeView x:Name="treeView1" Margin="10"/>
    </Grid>
</Window>
